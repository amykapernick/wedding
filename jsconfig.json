{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"skipLibCheck": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
			"name": "next"
			}
		],
		"paths": {
			"@components/*": ["./src/components/*"],
			"@parts/*": ["./src/components/parts/*"],
			"@styles/*": ["./src/styles/*"],
		}
	},
	"exclude": ["node_modules"]
}